<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.TabPane?>
<?import com.automation.device.controllers.*?>
<?import com.automation.device.controllers.ScreenController?>
<?import com.automation.device.controllers.AppsController?>
<?import com.automation.device.controllers.LogController?>

<fx:root type="TabPane" id="deviceContent" tabMinWidth="80.0" xmlns:fx="http://javafx.com/fxml"
	style=" -fx-border-color:black; -fx-border-width: 2; -fx-border-style: solid;">
    <tabs>
		<Tab text="SCREEN" fx:id="screenTab" closable="false">
			<ScreenController fx:id="screenCtrl"/>
		</Tab>
	    <Tab text="APPS" fx:id="appsTab" closable="false">
	    	<AppsController fx:id="appsCtrl"/>
	    </Tab>
	    <Tab text="LOG" fx:id="logTab" closable="false">
	    	<LogController fx:id="logCtrl"/>
	    </Tab>
	</tabs>
</fx:root>